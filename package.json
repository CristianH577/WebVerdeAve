{
  "name": "verde-ave-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nextui-org/react": "^2.2.9",
    "@react-stately/data": "^3.10.3",
    "aos": "^2.3.4",
    "axios": "^1.6.2",
    "formik": "^2.4.5",
    "framer-motion": "^10.18.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.1",
    "react-icons": "^5.0.1",
    "react-leaflet": "^4.2.1",
    "react-lorem-ipsum": "^1.4.10",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "react-transition-group": "^4.4.5",
    "swiper": "^11.0.7",
    "use-dark-mode": "^2.3.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm:start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^30.0.8",
    "electron-builder": "^24.13.3",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.3.3",
    "wait-on": "^7.2.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "Cristian Herrera",
    "email": "cristian.herrera07@hotmail.com",
    "url": "https://github.com/CristianH577"
  },
  "build": {
    "appId": "verde.ave.app.id",
    "productName": "VerdeAve",
    "copyright": "Copyright Â© 2024 VerdeAve",
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "public/electron.js"
    ],
    "extraResources": [
      {
        "from": "public/assets/",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "icon": "public/assets/ave.ico"
    },
    "mac": {
      "icon": "public/assets/logo.png"
    },
    "linux": {
      "icon": "public/assets/logo.png"
    }
  }
}
